{
  "name": "frazewise",
  "version": "0.0.0",
  "uiVersion": "0.0.6",
  "author": "Peter Kin",
  "scripts": {
    "start:api": "tsx ./api/server.ts",
    "serve:api:docker:build": "docker-compose -f ./api/docker/api.docker-compose.yml up --build",
    "serve:api:docker": "docker-compose -f ./api/docker/api.docker-compose.yml up",
    "build:check:api": "tsc --noEmit",
    "build-publish:api": "node api/scripts/build-publish.mjs",
    "lint:api": "eslint ./api --ext .ts",
    "start:db": "docker-compose -f api/docker/dev-db.docker-compose.yml up",
    "clear:db": "docker-compose -f api/docker/dev-db.docker-compose.yml down -v",
    "start:ui:dev": "npm run setenv && ng serve",
    "start:ui:prod": "npm run setenv && ng serve --configuration production",
    "build:ui": "npm run setenv && ng build",
    "test:ui": "ng test",
    "lint:ui": "ng lint",
    "lint": "npm run lint:ui && npm run lint:api",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "checks": "npm run lint && npm run format:check && npm run build:check:api && npm run build:ui && npm run check-dependencies",
    "check-dependencies": "node api/scripts/check-dependencies.mjs",
    "setenv": "node ui/setenv.cjs",
    "ng": "ng",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=22.5.1"
  },
  "private": true,
  "type": "module",
  "dependencies": {
    "@angular/animations": "^19.1.0",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/forms": "^19.1.0",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-browser-dynamic": "^19.1.0",
    "@angular/router": "^19.1.0",
    "@google/generative-ai": "^0.22.0",
    "@ng-icons/core": "^30.3.0",
    "@ng-icons/tabler-icons": "^30.3.0",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "date-fns": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.13.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.8",
    "@angular/cli": "^19.1.8",
    "@angular/compiler-cli": "^19.1.0",
    "@types/jasmine": "~5.1.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.5",
    "@types/uuid": "^10.0.0",
    "angular-eslint": "19.1.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.5.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "8.23.0"
  }
}
